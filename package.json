{
  "name": "knex-node-starter",
  "version": "1.0.0",
  "description": "Node knex redux redis starter",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "node_modules/.bin/knex migrate:latest --env server_env",
    "start": "node_modules/.bin/pm2 start server/pm2_configs/pm2_config.json",
    "start:dev": "node_modules/.bin/pm2 start server/pm2_configs/pm2_config.json --watch",
    "build:dev": "node_modules/.bin/webpack -w",
    "build": "NODE_ENV='production' node_modules/.bin/webpack -p",
    "eslint": "git ls-files | grep -v '^tests' | grep '\\.js$' | xargs eslint --config .eslintrc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nitish24p/knex-node-starter.git"
  },
